<div class="arrow">
    <img src="../img/left.png" alt="">
</div>
<div class="right-part">
    <form class="filters" action="">
        <div class="filters__title">Фильтры</div>
        <div class="filters__item audience">
            <h3 class="title">Целевая аудитория</h3>
            <div class="aud__checkboxes dropdown">
                <div class="audience__checkbox">
                    <input type="checkbox" id="employees" name="employee">
                    <label for="employees">Сотрудники</label>
                </div>
                <div class="audience__checkbox">
                    <input type="checkbox" id="students" name="student">  <label for="students">Студенты</label>
                </div>
                <div class="audience__checkbox">
                    <input type="checkbox" id="abiturients" name="abiturient">
                    <label for="abiturients">Абитуриенты</label>  
                </div>
                <div class="audience__checkbox">
                    <input type="checkbox" id="graduates" name="graduate"> <label for="graduates">Выпускники</label> 
                </div>
                <div class="audience__checkbox">
                    <input type="checkbox" id="employers" name="employer"> <label for="employers">Работодатели</label> 
                </div>
            </div>
        </div>
        <div class="filters__item nickname">
            <h3 class="title">Никнейм</h3>
            <input class="dropdown" type="text" placeholder="Никнейм">
        </div>
        <div class="filters__item emotions" name="uncertain">
            <h3 class="title">Эмоциональная окраска</h3>
            <div class="em__checkboxes dropdown">
                <div class="emotions__checkbox">
                    <input type="checkbox" id="positive" name="positive">  
                    <label for="positive">Позитивная (+)</label>
                </div>
                <div class="emotions__checkbox">
                    <input type="checkbox" id="negative" name="negative">  
                    <label for="negative">Негативная (-)</label>
                </div>
                <div class="emotions__checkbox">
                    <input type="checkbox" id="neutral" name="neutral"> 
                    <label for="neutral">Нейтральная (*)</label> 
                </div>
                <div class="emotions__checkbox">
                    <input type="checkbox" id="uncertain" name="uncertain">  
                    <label for="uncertain">Не определенная (<>)</label>
                </div>
            </div>
        </div>
        <div class="filters__item keywords">
            <h3 class="title">Ключевые слова</h3>
            <input class="dropdown ss" type="text" placeholder="Ключевые слова">
        </div>
        <div class="filters__item data">
            <h3 class="title">Промежуток времени</h3>
            <div class="interval dropdown">
                <div class="from">
                    <label for="from">От: </label>
                    <input type="date" id="from" name="data" min="2017-07-20" max="2017-10-20">
                </div>
                <div class="to">
                    <label for="to">До: </label>
                    <input type="date" id="to" name="data" min="2017-07-20" max="2017-10-20">
                </div>
            </div>
        </div>
        <div class="filters__button">
            <input type="reset" class="fil-b reset">
            <div class="fil-b submit">Принять</div>
        </div>
    </form>
</div>
</div>
</div>
</div>

